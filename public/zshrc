export DOTFILES_PATH="/Users/danieluhl/git/dotfiles/public"

source $DOTFILES_PATH/env.sh

# Load z
[ -f $(brew --prefix)/etc/profile.d/z.sh ] && source $(brew --prefix)/etc/profile.d/z.sh

# Load aliases
[[ -f ~/.aliases ]] && source ~/.aliases

# Local config
[[ -f ~/.zshrc.local ]] && source ~/.zshrc.local

# Load git completion
[[ -f ~/.git-completion.sh ]] && source ~/.git-completion.sh

autoload -Uz compinit
compinit

# Activate rtx for the session (rust based pro)
eval "$(/opt/homebrew/bin/rtx activate zsh)"

autoload -U +X bashcompinit && bashcompinit

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/duhl/google-cloud-sdk/path.zsh.inc' ]; then . '/Users/duhl/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/duhl/google-cloud-sdk/completion.zsh.inc' ]; then . '/Users/duhl/google-cloud-sdk/completion.zsh.inc'; fi

# source /Users/danieluhl/.docker/init-zsh.sh || true # Added by Docker Desktop

source $DOTFILES_PATH/login.sh

eval "$(starship init zsh)"

clear
